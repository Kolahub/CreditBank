{
    "builds": [
      {
        "src": "scripts/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/*",
        "use": "@vercel/static"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  